{
  "name": "react-time-range-selector",
  "version": "0.1.8",
  "description": "time range selector",
  "author": "wsafight",
  "license": "MIT",
  "main": "dist/index.js",
  "private": false,
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 9001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LazierGame/react-time-range-selector.git"
  },
  "keywords": [
    "time-range",
    "selector"
  ],
  "bugs": {
    "url": "https://github.com/LazierGame/react-time-range-selector/issues"
  },
  "homepage": "https://github.com/LazierGame/react-time-range-selector#readme",
  "dependencies": {
    "re-resizable": "^6.9.0",
    "react-dnd": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "lodash.throttle": "^4.1.1",
    "react-hammerjs": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.1.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3",
    "@storybook/react": "^6.1.14",
    "storybook-mobile": "^0.1.29"
  },
  "files": [
    "dist"
  ]
}
