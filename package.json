{
  "name": "react-mobile-range-selector",
  "version": "0.3.0",
  "description": "mobile range selector",
  "author": "wsafight",
  "license": "MIT",
  "main": "dist/index.js",
  "private": false,
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LazierGame/react-mobile-range-selector.git"
  },
  "keywords": [
    "time-range",
    "selector"
  ],
  "bugs": {
    "url": "https://github.com/LazierGame/react-mobile-range-selector/issues"
  },
  "homepage": "https://github.com/LazierGame/react-mobile-range-selector#readme",
  "files": [
    "dist"
  ],
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/react-mobile-range-selector.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-mobile-range-selector.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "re-resizable": "^6.9.0",
    "react-dnd": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "lodash.throttle": "^4.1.1",
    "alloyfinger": "^0.1.16"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@size-limit/preset-small-lib": "^5.0.2",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addons": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "husky": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
